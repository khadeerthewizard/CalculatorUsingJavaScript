{
    "manifest_version": 2,
    "name": "Browser",
    "description": "This extension will let you calculate",
    "version": "1",
    "author": "Khadeer",
    "permissions": [
        "activeTab",
        "http://*.google.com/"
    ],
    "browser_action": {
        "default_popup": "index.html",
        "default_title": "Calculator"
    },
    "icons": {
        "16": "imgs/logo.png",
        "48": "imgs/logo.png",
        "128": "imgs/logo.png"
    }
}
